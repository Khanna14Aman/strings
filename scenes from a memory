question==>https://codeforces.com/contest/1562/problem/B
editorial==>https://codeforces.com/blog/entry/94278
#include<bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    int i,j;
    bool prime[100];
    for(i=0;i<100;i++){
        prime[i]=true;
    }
    for(i=2;i*i<=100;i++){
        if(prime[i]){
            for(j=i*i;j<=100;j+=i){
                prime[j]=false;
            }
        }
    }
    while(t--){
        int n;
        cin>>n;
        string s;
        cin>>s;
        int flag=0;
        for(i=0;i<n;i++){
            if(s[i]=='1' || s[i]=='4' || s[i]=='6' || s[i]=='8' || s[i]=='9'){
                cout<<"1\n";
                cout<<s[i]<<"\n";
                flag=1;
                break;
            }
        }
        if(flag==1){
            continue;
        }
        for(i=0;i<n;i++){
            for(j=i+1;j<n;j++){
                if(!prime[(s[i]-'0')*10+(s[j]-'0')]){
                    cout<<"2\n";
                    cout<<s[i]<<s[j]<<"\n";
                    flag=1;
                    break;
                }
            }
            if(flag==1){
                break;
            }
        }
        
    }
    return 0;
}
