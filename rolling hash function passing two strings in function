#include<iostream>
#include<bits/stdc++.h>
#define int long long 
using namespace std;
pair<int,int> ha(string s1, string s2)
{
    int hash1=0,hash2=0,pr1=1,pr2=1,mod1 = (int)(1e9+7);
    int mod2 = (int)(1e9+3),n=s1.length(),m=s2.length();
    int i; 
    for(i=0;i<n;i++)
    {
        hash1 = (hash1 + ((s1[i]-'a'+1)*pr1)%mod1)%mod1;
        pr1 = (pr1 * 31)%mod1;
    }
    for(i=0;i<m;i++)
    {
        hash2 = (hash2 + ((s2[i]-'a'+1)*pr2)%mod2)%mod2;
        pr2 = (pr2 * 29);
    }
    return {hash1,hash2};
}
signed main()
{
    string s1,s2;
    pair<int,int>p;
    cout<<"\nenter string s1 and s2\n";
    cin>>s1>>s2;
    p = ha(s1,s2);
    cout<<p.first<<" "<<p.second;
    return 0;
}
