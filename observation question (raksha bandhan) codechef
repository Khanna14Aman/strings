question==>https://www.codechef.com/problems/INSQ15_A
#include<iostream>
#include<bits/stdc++.h>
#define int long long 
using namespace std;
char ss[10000007];
vector<int> ze(string s)
{
    int i = 1,l=0,r=0,n=s.length();
    vector<int>ze(n,0);
    while(i<n)
    {
        if(i<=r)
        {
            ze[i] = min(ze[i-l],r-i+1);
        }
        while((i+ze[i])<n && s[i+ze[i]]==s[ze[i]])
            ze[i]++;
        if((i+ze[i]-1)>r)
        {
            l=i;
            r=i+ze[i]-1;
        }
        i++;
    }
    return ze;
}
signed main()
{
    scanf("%s",ss);  //for entering string with the use of scanf we need to initialize string as character we cannot declare string using " string " keyword
    vector<int>vec;
    vec = ze(ss);
    int q;
    scanf("%lld",&q);
    while(q--)
    {
        int no;
        scanf("%lld",&no);
        printf("%lld\n",min(no,vec[no]));
    }
    return 0;
}
