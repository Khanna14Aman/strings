question==>https://www.spoj.com/problems/NHAY/
#include<iostream>
#include<bits/stdc++.h>
#define int long long 
using namespace std;
int f=0;
vector<int> ze(string s)
{
    int i = 1,l=0,r=0,n=s.length();
    vector<int>ze(n,0);
    while(i<n)
    {
        if(i<=r)
        {
            ze[i] = min(ze[i-l],r-i+1);
        }
        while((i+ze[i])<n && s[i+ze[i]]==s[ze[i]])
            ze[i]++;
        if((i+ze[i]-1)>r)
        {
            l=i;
            r=i+ze[i]-1;
        }
        i++;
    }
    return ze;
}
void zee()
{
    string s,pat;
    int no;
    cin>>pat>>s;
    int n = s.length(),i;
    no = pat.length();
    vector<int>vec = ze(pat+"$"+s);
    int count=0, k = vec.size(),flag=0;
    for(i=0;i<k;i++)
    {
        if(vec[i]==no)
        {
            flag=1;
            cout<<i-no-1<<"\n";
        }
    }
    if(flag==0)
    {
        cout<<"\n";
    }

}
signed main()
{
    int no;
    while(cin>>no)//very important
    {
         zee();
    }
    return 0;
}
