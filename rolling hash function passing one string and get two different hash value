#include<iostream>
#include<bits/stdc++.h>
#define int long long 
using namespace std;
pair<int,int> ha(string s)
{
    int hash1=0,hash2=0,pr1=1,pr2=1,mod1 = (int)(1e9+7);
    int mod2 = (int)(1e9+3),n=s.length();
    int i; 
    for(i=0;i<n;i++)
    {
        hash1 = (hash1 + ((s[i]-'a'+1)*pr1)%mod1)%mod1;
        hash2 = (hash2 + ((s[i]-'a'+1)*pr2)%mod2)%mod2;
        pr1 = (pr1 * 31)%mod1;
        pr2 = (pr2 * 32)%mod2;
    }
    return {hash1,hash2};
}
signed main()
{
    string s;
    pair<int,int>p;
    cout<<"\nenter string s \n";
    cin>>s;
    p = ha(s);
    cout<<p.first<<" "<<p.second;
    return 0;
}
